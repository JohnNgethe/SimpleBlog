{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*).(js|css|png|jpg|jpeg|gif|ico|json|svg|woff2|woff|ttf|eot)",
      "headers": {
        "Cache-Control": "s-maxage=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/views/partials/(.*)",
      "dest": "/views/partials/$1"
    },
    {
      "src": "/views/(.*)",
      "dest": "/views/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "app.js",
      "cleanUrls": true,
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
